<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="AsistenteUCAB.Modelos" assembly="AsistenteUCAB" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Actividad" lazy="true" table="`ACTIVIDAD`" schema="`dbo`">
		<id name="IdActividad" access="property" column="`idActividad`">
			<generator class="native" />
		</id>
		<property name="Expediente" column="`expediente`" />
		<property name="HoraFin" column="`horaFin`" />
		<property name="HoraInicio" column="`horaInicio`" />
		<property name="IdMateria" column="`idMateria`" />
    <property name="Nombre" column="`nombre`" length="50" />
    <property name="Seccion" column="`seccion`" length="10" />
		<property name="Periodo" column="`periodo`" length="10" />
	</class>
</hibernate-mapping>
